{
  "name": "@storeon/undo",
  "version": "0.2.1",
  "description": "Module for storeon which allows undoing or redoing the latest event",
  "repository": "storeon/undo",
  "author": "Ivan Menshykov <ivan.menshykov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint *.js test/*.js test/demo/*.js",
    "size": "size-limit",
    "jest": "NODE_ENV=development jest --coverage",
    "spell": "yarn docs && yaspeller *.md",
    "docs": "documentation build *.js -f md -o api.md",
    "test": "yarn jest && yarn lint && size-limit && yarn spell",
    "start": "parcel serve --cache-dir test/demo/.cache -d test/demo/dist test/demo/index.html --open"
  },
  "devDependencies": {
    "@logux/eslint-config": "^28.2.2",
    "clean-publish": "^1.1.2",
    "documentation": "^11.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-import-helpers": "^1.0.1",
    "eslint-plugin-jest": "^22.7.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "size-limit": "^1.3.7",
    "storeon": "^0.8.5",
    "yaspeller": "^5.1.0"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "321 B"
    },
    {
      "path": "full.js",
      "limit": "354 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser",
    "rules": {
      "es5/no-es6-static-methods": "off",
      "consistent-return": "off",
      "func-style": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "*.js": ["yarn jest", "yarn lint", "size-limit"],
    "*.md": "yaspeller"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "dictionary": [
      "storeon",
      "Versioning",
      "undoable",
      "createHistory"
    ]
  }
}
