{
  "name": "@storeon/undo",
  "version": "0.1.0",
  "description": "Module for storeon to add undo and redo functionaly of the state",
  "repository": "storeon/undo",
  "author": "Ivan Menshykov <ivan.menshykov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint *.js test/*.js",
    "size": "size-limit",
    "spell": "yarn docs && yaspeller *.md",
    "docs": "documentation build *.js -f md -o api.md",
    "test": "jest --coverage && yarn lint && size-limit && yarn spell"
  },
  "devDependencies": {
    "@logux/eslint-config": "^28.2.0",
    "clean-publish": "^1.1.2",
    "documentation": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-import-helpers": "^0.1.4",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.5",
    "parcel-bundler": "^1.12.3",
    "react": "^16.0.0",
    "react-dom": "^16.8.6",
    "size-limit": "^1.3.0",
    "storeon": "^0.8.0",
    "yaspeller": "^5.0.1"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "196 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser",
    "rules": {
      "es5/no-es6-static-methods": "off",
      "consistent-return": "off",
      "func-style": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.md": "yaspeller"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "dictionary": [
      "storeon",
      "Versioning",
      "undoable"
    ]
  }
}
