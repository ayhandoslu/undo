{
  "name": "@storeon/undo",
  "version": "0.5.0",
  "description": "Module for storeon which allows undoing or redoing the latest event",
  "repository": "storeon/undo",
  "author": "Ivan Menshykov <ivan.menshykov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint *.js test/*.js test/demo/*.js",
    "size": "size-limit",
    "jest": "NODE_ENV=development jest --coverage test/*.js",
    "spell": "yarn docs && yaspeller *.md",
    "docs": "documentation build *.js -f md -o api.md",
    "test": "yarn jest && yarn lint && size-limit && yarn spell",
    "start": "parcel serve --cache-dir test/demo/.cache -d test/demo/dist test/demo/index.html --open"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.1",
    "@logux/eslint-config": "^36.1.2",
    "@size-limit/preset-small-lib": "^4.4.5",
    "clean-publish": "^1.1.7",
    "documentation": "^12.3.0",
    "dual-publish": "^0.10.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^19.0.0",
    "jest": "^25.4.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "size-limit": "^4.4.5",
    "storeon": "^2.0.2",
    "yaspeller": "^7.0.0"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "320 B"
    },
    {
      "path": "full/index.js",
      "limit": "356 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config",
    "rules": {
      "es5/no-es6-static-methods": "off",
      "consistent-return": "off",
      "func-style": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "(index|full).js": [
      "yarn jest",
      "yarn lint",
      "yarn size"
    ],
    "test/*.js": [
      "yarn jest",
      "yarn lint"
    ],
    "*.md": "yaspeller"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "dictionary": [
      "storeon",
      "Versioning",
      "undoable",
      "createHistory",
      "eslint"
    ]
  }
}
